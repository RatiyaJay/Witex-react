PORT=4000
NODE_ENV=development

# CORS allowed origins (comma-separated). Example: http://localhost:3000,https://witex.in
CORS_ORIGINS=http://localhost:3000

# Discrete DB variables
DB_NAME=your_db_name
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_HOST=your_db_host
DB_DIALECT=mysql

# Or single URL used by Sequelize CLI (config.json)
# mysql://USER:PASSWORD@HOST/DBNAME
DATABASE_URL=mysql://your_db_user:your_db_password@your_db_host/your_db_name
